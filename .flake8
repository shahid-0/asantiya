[flake8]
max-line-length = 88
extend-ignore = E203,E501,W503,E402,C901
exclude = 
    .git,
    __pycache__,
    .venv,
    venv,
    .pytest_cache,
    build,
    dist,
    *.egg-info,
    .coverage,
    htmlcov,
    .mypy_cache,
    .tox
per-file-ignores =
    __init__.py:F401
max-complexity = 10
select = C,E,F,W,B,B950
